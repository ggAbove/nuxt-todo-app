    "update": "bunx --bun nuxi upgrade --force"
  },
  "dependencies": {
    "@nuxt/eslint": "0.7.4",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/tailwindcss": "6.12.2",
    "@pinia/nuxt": "^0.9.0",
    "nuxt": "^3.14.1592",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "eslint": "^9.17.0",
    "typescript": "^5.7.2"
  }
